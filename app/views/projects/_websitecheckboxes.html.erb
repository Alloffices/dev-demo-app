<!-- Checkbox One ////////////////////////////////////////////////////// -->
<div class="checkbox check-primary relative-for-checkboxes">
  <input type="checkbox" value="1" id="checkbox2" class="testing">
  <label for="checkbox2">Build Home Page (Refer to Mockup)</label>
  <div class="check-boxes-todo-list">
    <%= form_with(model: @project, remote: true, local: true) do |form| %>
      <div>
        <div id="info_true">
          <%= form.radio_button :build_one, false, :checked => true, :value => false, id: "build_one_1", :style => "visibility:hidden"  %>
          <%= form.radio_button :build_one, true, :value => true, id: "build_one_2", :style => "visibility:hidden" %>
        </div>
      </div>
      <%= form.submit id: "build_one", :style => "visibility:hidden;", data: { confirm: "Are you sure?"} %>
    <% end %>
  </div>
</div>


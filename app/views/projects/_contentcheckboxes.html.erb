<!-- Checkbox One ////////////////////////////////////////////////////// -->
<div class="checkbox check-primary relative-for-checkboxes">
  <input type="checkbox" value="1" id="checkbox11" class="testing_10">
  <label for="checkbox11">*****( Providing Content )*****</label>
  <div class="check-boxes-todo-list">
    <%= form_with(model: @project, local: true) do |form| %>
      <div>
        <div id="info_true">
          <%= form.radio_button :build_ten, false, :checked => true, :value => false, id: "build_ten_10_1", :style => "visibility:hidden"  %>
          <%= form.radio_button :build_ten, true, :value => true, id: "build_ten_10_2", :style => "visibility:hidden" %>
        </div>
      </div>
      <%= form.submit id: "build_ten", :style => "visibility:hidden;", data: { confirm: "Are you sure?"} %>
    <% end %>
  </div>
</div>

